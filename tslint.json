{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "dist",
      "build"
    ]
  },
  "rules": {
    "variable-name": false,
    "quotemark": false,
    "no-console": false,
    "ordered-imports": false,
    "jsx-no-lambda": false
  },
  "rulesDirectory": [],
  "defaultSeverity": "warning"
}
